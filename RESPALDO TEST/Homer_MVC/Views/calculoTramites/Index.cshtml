                    $('#vr_traspaso_c').val(data[0].traspaso);
                    $('#vr_traspaso_d').val(data[0].traspaso * 2);

                    if ($('#tipo_analisis').val() == "doble") {
                        $('#vr_tarjeta').val(0);
                    }
                    else {
                        $('#vr_tarjeta').val(data[0].tarjeta);
                    }
                    $('#vr_tarjeta_c').val(data[0].tarjeta);
                    $('#vr_tarjeta_d').val($('#vr_tarjeta_c').val() + $('#vr_tarjeta').val());

                    $('#vr_consgMin').val(data[0].consig_minist_tte);
