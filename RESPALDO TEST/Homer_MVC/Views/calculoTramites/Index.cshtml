            //    ]
            //});
        });

        $('.js-source-states').select2();

        $('#fecha_tramite').change(function () {
            $('#fecha_factura').val($('#fecha_tramite').val())
        });

        $('.soat').change(function () {
            calcularVrSoat();
        });

        $('.tecno').change(function () {
            carcularVrTecnomecanica();
        });

        $('.sumaDoble').change(function () {
            sumarValores()
        });

        $('.buscar').change(function () {
            buscarValores()
        });

        $('#avaluo').change(function () {
            var avaluo = (parseFloat($('#avaluo').val()) * 1) / 100
            $('#vr_retefuente').val(avaluo)
        });

        $('#vr_retefuente').change(function () {
            $('#vr_retefuente_d').val(parseFloat($('#vr_retefuente').val()) + parseFloat($('#vr_retefuente_c').val()))
        });

        $('#vr_retefuente_c').change(function () {
            $('#vr_retefuente_d').val(parseFloat($('#vr_retefuente').val()) + parseFloat($('#vr_retefuente_c').val()))
        });

        $('#tipo_analisis').change(function () {
            if ($('#tipo_analisis').val() == "comprador") {
                $('.vendedor').hide()
                $('.comprador').show()
                $('.doble').hide()
            }
            if ($('#tipo_analisis').val() == "vendedor") {
                $('.vendedor').show()
                $('.comprador').hide()
                $('.doble').hide()
            }
            if ($('#tipo_analisis').val() == "doble") {
                $('.vendedor').show();
                $('.comprador').show();
                $('.doble').show();
            }
        });

        function calcularVrSoat() {

            if ($('#soat').val() == "si")
            {
                var fecha1 = moment($('#fecha_factura').val());
                var fecha2 = moment($('#fecha_fin_soat').val());
                var soat = $('#valor_soat').val() / 12

                //vendedor
                var cantDias2 = (fecha1.diff(fecha2, 'days')) / 30
                var total = Math.round((soat * cantDias2) / 1000) * 1000
                $('#vr_soat').val(addComas(total))

                //comprador
                var cantDias = (fecha2.diff(fecha1, 'days')) / 30
                var total2 = Math.round((soat * cantDias) / 1000) * 1000
                $('#vr_soat_c').val(addComas(total2))

                //doble
                $('#vr_soat_d').val(total + total2)
            }
            else
            {
                $('#vr_soat').val(0);
                $('#vr_soat_c').val(0);
            }
            sumarValores();
        }

        function carcularVrTecnomecanica() {

            var fechaInicial = moment($('#fecha_inicial_tecnomecanica').val());
            var fechaFin = moment($('#fecha_final_tecnomecanica').val());
            var fechaFactura = moment($('#fecha_factura').val());

            //comprador
            var cantDias = (fechaFin.diff(fechaFactura, 'days')) / 30;
            var cantDias2 = (fechaFin.diff(fechaInicial, 'days')) / 30;
            var valor = parseFloat($('#valor_tecnomecanica').val() / cantDias2);
            if (fechaFin < fechaFactura)
            {
                var total = 0;
            }
            else
            {
                var total = Math.round((valor * cantDias) / 1000) * 1000;
            }
            $('#vr_tecnomecanica_c').val(addComas(total));

            //vendedor
            var cantDias_c = (fechaFactura.diff(fechaFin, 'days')) / 30;
            var cantDias2_c = (fechaFin.diff(fechaInicial, 'days')) / 30;
            var valor_c = parseFloat($('#valor_tecnomecanica').val() / cantDias2_c);
            var total_c = Math.round((valor_c * cantDias_c) / 1000) * 1000;
            $('#vr_tecnomecanica').val(addComas(total_c))

            //doble
            $('#vr_tecnomecanica_d').val(total_c + total)

            sumarValores();
        }

        function buscarValores() {

            $.ajax({
                url: '/calculoTramites/BuscarValores',
