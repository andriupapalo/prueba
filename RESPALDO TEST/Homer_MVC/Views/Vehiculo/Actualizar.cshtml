@model Homer_MVC.IcebergModel.CrearVehiculoModel

@{
    ViewBag.Icono = "fa fa-car";
    ViewBag.Title = "Vehiculo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles {
    @Styles.Render("~/bundles/select2/css")
    <link href="~/Vendor/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Vendor/bootstrap-toggle-master/css/bootstrap-toggle.min.css" rel="stylesheet" />
    <link href="~/Vendor/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
}

<div class="panel-body">
    <div class="panel-heading" style="background-color:white;  border:solid 1px; border-color:#e4e5e7">
        <div class="panel-body">

            <div id="hbreadcrumb" class="pull-right">

                <button type="button" class="btn btn-mini btn-default" style="border-radius:25px" data-container="body" data-toggle="popover" data-placement="left" data-title="Enlaces" data-content="<ul>@ViewBag.nombreEnlaces</ul>" data-html="true">
                    <i class="text-info fa fa-tags"></i>
                </button>
                <button type="button" class="btn btn-mini btn-default" style="border-radius:25px" data-container="body" data-toggle="popover" data-placement="left" data-title="Documentos" data-content="<ul></i><a href=''>Documento 1</a></ul>  <ul></i><a href=''>Documento 2</a></ul> <ul></i><a href=''>Documento 3</a></ul>" data-html="true">
                    <i class="text-primary fa fa-files-o"></i>
                </button>
                <button type="button" class="btn btn-mini btn-default" style="border-radius:25px" data-container="body" data-toggle="popover" data-placement="left" data-title="Favoritos" data-content="@ViewBag.Favoritos" data-html="true">
                    <i class="text-warning fa fa-star"></i>
                </button>

            </div>
            <h3 class="font-light m-b-xs">
                <i class="text-info @ViewBag.Icono"></i>&nbsp;&nbsp;@ViewBag.Title
            </h3>
            <small>Actualizar @ViewBag.Title</small>
        </div>
    </div>
</div>

<div class="panel-body" style="padding-top:0px;">
    <div class="hpanel">
        @if (TempData["mensaje"] != null)
        {
            <div class="alert alert-success  alert-dismissible" id="mensaje">
                <button type="button" class="close" data-dismiss="alert" arial-label="close"><span aria-hidden="true">&times;</span></button>
                <p>@TempData["mensaje"]</p>
            </div>
        }

        @if (TempData["mensaje_error"] != null)
        {
            <div class="alert alert-danger  alert-dismissible" id="mensaje_error">
                <button type="button" class="close" data-dismiss="alert" arial-label="close"><span aria-hidden="true">&times;</span></button>
                <p>@TempData["mensaje_error"]</p>
            </div>
        }

        <div id="tabs" class="tab-content">

            <div class="m float-e-margins text-right">
                <div class="tooltip-demo">

                    <div id="div-mensaje"></div>

                </div>
            </div>

            <ul id="tabs-crear" class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#crear"><i class="@ViewBag.Icono"></i>&nbsp;&nbsp;@ViewBag.Title</a></li>
                <li class="" onclick="buscarAjaxVehiculos();"><a data-toggle="tab" href="#buscar"><i class="fa fa-search"></i>&nbsp;&nbsp;Búsquedas</a></li>
            </ul>

            <div id="crear" class="tab-pane active">
                <div class="panel-body">

                    @using (Html.BeginForm())
                    {
                        <div class="panel-body-btns text-right">
                            <a class="btn btn-info" href="../../creacionVh?menu=@ViewBag.id_menu"><i class="@ViewBag.Icono"></i>&nbsp;&nbsp;&nbsp;Crear nuevo</a>&nbsp;&nbsp;
                            <button class="btn btn-info" type="submit" id="btnActualizar"><i class="fa fa-save"></i>&nbsp;&nbsp;&nbsp;Actualizar</button>

                        </div>

                        @Html.AntiForgeryToken()
                        @Html.Hidden("menu")
                        @Html.HiddenFor(model => model.icbvh_id)
                        @Html.HiddenFor(model => model.icbvhid_licencia)
                        @Html.HiddenFor(model => model.icbvhfec_creacion)
                        @Html.HiddenFor(model => model.icbvhuserid_creacion)
                        @Html.HiddenFor(model => model.icbvhuserid_actualizacion)
                        @Html.HiddenFor(model => model.icbvhfec_actualizacion)

                        <div class="form-horizontal">

                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })


                            <div class="hpanel">
                                <div class="panel-heading hbuilt" style="background-color:#e4e5e7">
                                    <div class="panel-tools">
                                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>

                                    </div>
                                    <i class="fa fa-car"></i>&nbsp;&nbsp;&nbsp;Datos Vehiculo
                                </div>
                                <div class="panel-body">

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Vehiculo:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.plan_mayor, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite vehiculo", @readonly = "readonly", @required = "required" } })
                                                @Html.ValidationMessageFor(model => model.plan_mayor, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Serie:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.vin, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite serie", @required = "required" } })
                                                @Html.ValidationMessageFor(model => model.vin, "", new { @class = "text-danger" })
                                                <label id="alertaSerieRepetida" class="text-danger" style="display:none;">La serie no puede ser igual al vehiculo</label>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Placa:&nbsp;</label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.plac_vh, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite placa" } })
                                                @Html.ValidationMessageFor(model => model.plac_vh, "", new { @class = "text-danger" })
                                                @*<label id="alertaPlacaRepetida" class="text-danger" style="display:none;">La placa no puede ser igual al vehiculo</label>*@
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6" style="display:none" id="placaCiudad" name="placaCiudad">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Ciudad Placa:</label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.ciudadplaca, ViewBag.ciudadplaca.Items as List<SelectListItem>, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione" })
                                                @Html.ValidationMessageFor(model => model.ciudadplaca, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Motor:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.nummot_vh, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite numero motor", @required = "required" } })
                                                @Html.ValidationMessageFor(model => model.nummot_vh, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Marca:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.marcvh_id, ViewBag.marcvh_id.Items as List<SelectListItem>, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", @required = "required" })
                                                @Html.ValidationMessageFor(model => model.marcvh_id, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Modelo:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.modvh_id, ViewBag.modvh_id.Items as List<SelectListItem>, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", @required = "required" })
                                                @Html.ValidationMessageFor(model => model.modvh_id, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Color:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.colvh_id, ViewBag.colvh_id.Items as List<SelectListItem>, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", @required = "required" })
                                                @Html.ValidationMessageFor(model => model.colvh_id, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Kilometraje:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.kilometraje, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite kilometraje", @onkeyUp = "return miles(this.id)", @required = "required" } })
                                                @Html.ValidationMessageFor(model => model.kilometraje, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">A&ntilde;o Modelo:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.anio_vh, Enumerable.Empty<SelectListItem>(), "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", @required = "required" })
                                                @Html.ValidationMessageFor(model => model.anio_vh, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Tipo:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.tpvh_id, ViewBag.tpvh_id.Items as List<SelectListItem>, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", @required = "required" })
                                                @Html.ValidationMessageFor(model => model.tpvh_id, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Tipo de Servicio:&nbsp;</label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.tipo_servicio, ViewBag.tipo_servicio.Items as List<SelectListItem>, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione" })
                                                @Html.ValidationMessageFor(model => model.tipo_servicio, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Tipo de Compra:&nbsp;</label>
                                            <div class="col-md-7">
                                                @Html.DropDownList("concepto", null, "Seleccione", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", })
                                                @Html.ValidationMessageFor(model => model.codigo_pago, "", new { @class = "text-danger" })
                                                <input name="textConcepto" id="textConcepto" class="form-control" type="hidden" value="">
                                                <input name="valConcepto" id="valConcepto" class="form-control" type="hidden" value="">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Nit de Prenda:&nbsp;</label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.nitprenda, null, "Seleccione", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", })
                                                @Html.ValidationMessageFor(model => model.codigo_pago, "", new { @class = "text-danger" })

                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Asesor:&nbsp;</label>
                                            <div class="col-md-7">
                                                @Html.DropDownListFor(model => model.idasesor, null, "Seleccione", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione", })
                                                @Html.ValidationMessageFor(model => model.idasesor, "", new { @class = "text-danger" })

                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Notas:&nbsp;</label>
                                            <div class="col-md-7">
                                                @Html.TextAreaFor(model => model.Notas, new RouteValueDictionary(new { @class = "someClass", style = "width: 100%; height:80px;", @placeholder = "Digite notas" }))
                                                @Html.ValidationMessageFor(model => model.Notas, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Es Flota:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.CheckBoxFor(model => model.flota, new { @data_toggle = "toggle", @data_size = "mini" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Es nuevo:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.CheckBoxFor(model => model.nuevo, new { @data_toggle = "toggle", @data_size = "mini" })

                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Es usado:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.CheckBoxFor(model => model.usado, new { @data_toggle = "toggle", @data_size = "mini" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="hpanel">
                                <div class="panel-heading hbuilt" style="background-color:#e4e5e7">
                                    <div class="panel-tools">
                                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>

                                    </div>
                                    <i class="fa fa-car"></i>&nbsp;&nbsp;&nbsp;Informacion de Costos
                                </div>
                                <div class="panel-body">

                                    <div class="row">
                                        <div class="col-sm-6" id="divNuevo">
                                            <label class="control-label col-md-4">Costo Sin Iva:&nbsp;<span class="text-danger">*</span></label>
                                            <div class="col-md-6">
                                                @Html.EditorFor(model => model.costosiniva_vh, new { htmlAttributes = new { @class = "form-control", @placeholder = "Costo sin iva", @onkeyUp = "return miles(this.id)", @required = "required", @readonly = "readonly" } })
                                                @Html.ValidationMessageFor(model => model.costosiniva_vh, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <label class="control-label col-md-4">% IVA Venta:&nbsp;</label>
                                            <div class="col-md-2">
                                                @Html.DropDownList("iva_vh", null, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "%", })
                                                @Html.ValidationMessageFor(model => model.iva_vh, "", new { @class = "text-danger" })
                                                <input name="textIVAVenta" id="textIVAVenta" class="form-control" type="hidden" value="">
                                                <input name="valIVAVenta" id="valIVAVenta" class="form-control" type="hidden" value="">
                                            </div>

                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6">
                                            <label class="control-label col-md-4">% IVA Compra:&nbsp;</label>
                                            <div class="col-md-2">
                                                @Html.DropDownListFor(model => model.iva_compra_vh, null, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "%", })
                                                @Html.ValidationMessageFor(model => model.iva_vh, "", new { @class = "text-danger" })
                                                <input name="textIVACompra" id="textIVACompra" class="form-control" type="hidden" value="">
                                                <input name="valIVACompra" id="valIVACompra" class="form-control" type="hidden" value="">
                                            </div>
                                            <div class="col-md-4">
                                                <input class="form-control" type="text" name="montoiva" id="montoiva" readonly="readonly" placeholder="Monto iva" , />
                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <label class="control-label col-md-4">% Impoconsumo:&nbsp;</label>
                                            <div class="col-md-2">
                                                @Html.DropDownList("impconsumo", null, "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "%", })
                                                @Html.ValidationMessageFor(model => model.impconsumo, "", new { @class = "text-danger" })
                                                <input name="text_impoconsumo" id="text_impoconsumo" class="form-control" type="hidden" value="">
                                                <input name="val_impoconsumo" id="val_impoconsumo" class="form-control" type="hidden" value="">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <label class="control-label col-md-4">Costo Total:&nbsp;&nbsp;&nbsp;</label>
                                            <div class="col-md-6">
                                                @Html.EditorFor(model => model.costototal_vh, new { htmlAttributes = new { @class = "form-control", @placeholder = "Valor con IVA incluido", @onkeyUp = "return miles(this.id)", @required = "required", @readonly = "readonly" } })
                                                @Html.ValidationMessageFor(model => model.costototal_vh, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>


                            <div class="hpanel">
                                <div class="panel-heading hbuilt" style="background-color:#e4e5e7">
                                    <div class="panel-tools">
                                        <a class="showhide"><i class="fa fa-chevron-up"></i></a>

                                    </div>
                                    <i class="fa fa-car"></i>&nbsp;&nbsp;&nbsp;Informaci&oacute;n Adicional
                                </div>
                                <div class="panel-body">

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Proveedor:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.DropDownList("proveedor_id", new SelectList(ViewBag.proveedor_id, "Value", "Text"), "", htmlAttributes: new { @class = "form-control js-source-states", @placeholder = "Seleccione" })
                                                @Html.ValidationMessageFor(model => model.proveedor_id, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Fecha Compra:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.fecfact_fabrica, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite serie" } })
                                                @Html.ValidationMessageFor(model => model.fecfact_fabrica, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Fecha Manifiesto:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.fecentman_vh, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite placa" } })
                                                @Html.ValidationMessageFor(model => model.fecentman_vh, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Fecha Matrícula:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
