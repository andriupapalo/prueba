                                            <label class="control-label col-md-4">Numero Manifiesto:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.nummanf_vh, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite numero manifiesto" } })
                                                @Html.ValidationMessageFor(model => model.nummanf_vh, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Numero Garantia:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.Numerogarantia, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite numero garantia", @type = "number" } })
                                                @Html.ValidationMessageFor(model => model.Numerogarantia, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-4">Fecha Garantia:&nbsp;<span class="text-danger"></span></label>
                                            <div class="col-md-7">
                                                @Html.EditorFor(model => model.fecha_garantia, new { htmlAttributes = new { @class = "form-control", @placeholder = "Digite fecha garantia" } })
                                                @Html.ValidationMessageFor(model => model.fecha_garantia, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">
                                        <div class="form-group">
