        function calcularMonto(){

            $('#textIVACompra').val($('#iva_compra_vh option:selected').text());
            $('#valIVACompra').val($('#iva_compra_vh option:selected').val());
            if ($('#costosiniva_vh').val() != null && $('#costosiniva_vh').val() != "") {
                if ( $('#iva_compra_vh').val() != null && $('#iva_compra_vh').val() != "") {

                    var a = parseInt($('#textIVACompra').val())
                    var b  = parseInt(quitCommas($('#costosiniva_vh').val()))
                    var opera  = ((a * b) / 100)
                    var opera2 = opera + b
                    $('#montoiva').val(addCommas(parseInt(opera)))
                    $('#costototal_vh').val(addCommas(parseInt(opera2)))
                }
            }
        }

        var numero_miles = "";
        var numero_miles2 = "";
        function formatNumber (n) {
            n = String(n).replace(/\D/g, "");
            return n === '' ? n : String(n).replace(/\B(?=(\d{3})+(?!\d))/g, ".");
        };
        function miles (id) {
            numero_miles = formatNumber($('#'+id+'').val());
            $('#'+id+'').val(numero_miles);
        };
        function formatNumber2 (n) {
            n = String(n).replace(/\D/g, "");
            return n === '' ? n : Number(n).toString();
        };
        function miles2 (id) {
            numero_miles2 = formatNumber2($('#'+id+'').val());
            $('#'+id+'').val(numero_miles2);
        };
