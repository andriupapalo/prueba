        function CalcularMargenUtilidad(bodega, referencia, cantidad, total, totaliva) {
            if (total <= 0) {
                mostrarMargenRefAgregado(false);
            } else {
                $.ajax({
                        url: '/inventario_repuesto/CalcularMargenU',
                        data: {
                            Bodega: bodega,
                            codigoref: referencia,
                            cantidad: cantidad,
                            valorTotal: total,
                            valoriva: totaliva
                        },
                        type: "post",
                        cache: false,
                        success: function (data) {
                            var total =  0;
                            var s = { Referencia: referencia, Margen: data };
                            MargenesUtilidad.push(s);
                            for (var i = 0; i < MargenesUtilidad.length; i++) {
                                total = parseFloat(total) + parseFloat(MargenesUtilidad[i]["Margen"]);
                            }
                            mostrarMargenRefAgregado(true, total);
                        }
                });
            }
        }

